{
  "name": "admincolumns",
  "version": "1.0.0",
  "description": "Sass and JS for Admin Columns",
  "private": true,
  "devDependencies": {
    "nps": "latest",
    "wp-pot": "latest",
    "babel-cli": "latest",
    "babel-preset-es2015": "latest",
    "node-sass": "latest",
    "rimraf": "latest",
    "webfont": "latest"
  },
  "scripts": {
    "ac:build:production": "nps build.production",
    "ac:build:development": "nps build.development",
    "ac:languages": "node languages.js",
    "ac:webfont": "nps webfont.build"
  },
  "webfont": {
    "formats": "woff",
    "template": "./webfont/template.scss.njk",
    "fontName": "cpac_icons",
    "fontHeight": "1000",
    "cssTemplateFontPath": "../fonts/",
    "cssTemplateClassName": "cpacicon",
    "normalize": true
  }
}
